{
  "npmScope": "hornts",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.js": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "core": {
      "tags": ["scope:core", "type:library"]
    },
    "common": {
      "tags": ["scope:common", "type:library"]
    },
    "http-express": {
      "tags": ["scope:http-express", "type:plugin"]
    },
    "http-fastify": {
      "tags": ["scope:http-fastify", "type:plugin"]
    }
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "packages"
  }
}
