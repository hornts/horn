{
  "version": 2,
  "projects": {
    "core": {
      "root": "packages/core",
      "sourceRoot": "packages/core/lib",
      "projectType": "library",
      "targets": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/core/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/core/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/packages/core"]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/core",
            "tsConfig": "packages/core/tsconfig.lib.json",
            "packageJson": "packages/core/package.json",
            "main": "packages/core/lib/index.ts",
            "assets": ["packages/core/*.md"],
            "buildableProjectDepsInPackageJsonType": "peerDependencies"
          },
          "outputs": ["{options.outputPath}"]
        }
      }
    },
    "common": {
      "root": "packages/common",
      "sourceRoot": "packages/common/lib",
      "projectType": "library",
      "targets": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/common/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/common/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/packages/common"]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/common",
            "tsConfig": "packages/common/tsconfig.lib.json",
            "packageJson": "packages/common/package.json",
            "main": "packages/common/lib/index.ts",
            "assets": ["packages/common/*.md"],
            "buildableProjectDepsInPackageJsonType": "peerDependencies"
          },
          "outputs": ["{options.outputPath}"]
        }
      }
    },
    "server-express": {
      "root": "packages/servers/server-express",
      "sourceRoot": "packages/servers/server-express/lib",
      "projectType": "library",
      "targets": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/servers/server-express/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/servers/server-express/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/packages/servers/server-express"]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/servers/server-express",
            "tsConfig": "packages/servers/server-express/tsconfig.lib.json",
            "packageJson": "packages/servers/server-express/package.json",
            "main": "packages/servers/server-express/lib/index.ts",
            "assets": ["packages/servers/server-express/*.md"],
            "buildableProjectDepsInPackageJsonType": "peerDependencies"
          },
          "outputs": ["{options.outputPath}"]
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
